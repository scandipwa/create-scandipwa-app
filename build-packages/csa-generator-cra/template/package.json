{
    "name": "<%= name %>",
    "description": "Clean project with minimum deps generated by CSA.",
    "version": "0.0.0",
    "scripts": {
        "start": "scandipwa-scripts start",
        "build": "scandipwa-scripts build"
    },
    "dependencies": {
        "react": "^<%= reactVersion %>",
        "react-dom": "^<%= reactDomVersion %>",
        "@scandipwa/scandipwa-scripts": "<%= scandipwaScriptsVersion %>",
        "@scandipwa/eslint-config": "<%= eslintConfigVersion %>",
        "@tilework/mosaic": "<%= mosaicVersion %>"
    },
    "scandipwa": {
        "themeAlias": "Blank",
        "type": "theme"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": "@scandipwa"
    },
    "license": "OSL-3.0",
    "publishConfig": {
        "access": "public"
    }
}
