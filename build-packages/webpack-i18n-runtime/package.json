{
    "name": "@scandipwa/webpack-i18n-runtime",
    "description": "Webpack internationalization plugin used used by CSA.",
    "version": "0.0.6",
    "license": "proprietary",
    "dependencies": {
        "@scandipwa/scandipwa-dev-utils": "0.0.20",
        "@scandipwa/webpack-after-emit-logger": "0.0.2"
    },
    "scandipwa": {
        "type": "extension",
        "extensions": {
            "@scandipwa/webpack-after-emit-logger": true
        },
        "build": {
            "cracoPlugins": [
                "build-config/config.plugin.js"
            ]
        }
    },
    "publishConfig": {
        "access": "public"
    }
}
